/**app.wxss**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.color-active{color: var(--theme)}
.my-button{
  background-color: var(--theme)!important;
  color: #fff;
}
.require{position:relative;}
.require::after{
  position: relative;
  right:-5rpx;
  top:0;
  content:'*';
  color:red;
}
.modal-button-line{
  display: flex;
  align-items: center;
  justify-content: center;
}
.modal-button-line button{
  width: 200rpx!important;
  margin: 0 20rpx!important;
}